[run]
source = backend/services/reactive_fabric_analysis
omit =
    */tests/*
    */__pycache__/*
    */venv/*
    */.venv/*
    */test_*

[report]
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    \.\.\.

[html]
directory = backend/services/reactive_fabric_analysis/htmlcov
