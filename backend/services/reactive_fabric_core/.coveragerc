[run]
source = .
omit = 
    tests/*
    */__pycache__/*
    */venv/*
    */.pyenv/*

[report]
fail_under = 25
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

