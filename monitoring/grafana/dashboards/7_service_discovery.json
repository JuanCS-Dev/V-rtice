        "y": 0,
        "w": 6,
        "h": 5
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(registry_operations_total{operation=\"register\"}[5m])) * 60",
          "legendFormat": "",
          "refId": "A"
