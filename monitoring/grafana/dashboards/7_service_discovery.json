{
  "uid": "vertice-service-discovery",
  "title": "7. Service Discovery",
  "tags": [
    "vertice",
    "discovery",
    "registration",
    "r5"
  ],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "panels": [
    {
      "id": 1,
      "title": "Total Registered Services",
      "type": "stat",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 5
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "count(registry_services_total)",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "textMode": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        }
      }
    },
    {
      "id": 2,
      "title": "Registrations/min",
      "type": "stat",
      "gridPos": {
